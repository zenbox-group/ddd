{
  "name": "zenbox/ddd",
  "description": "Domain Driven Design in PHP",
  "keywords": [
    "zenbox",
    "ddd",
    "extensions"
  ],
  "homepage": "https://github.com/zenbox-group/ddd",
  "license": "MIT",
  "type": "library",
  "require": {
    "php": "^8.1",
    "beberlei/assert": "^3.3",
    "ramsey/uuid": "^4.3",
    "laminas/laminas-code": "^4.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "suggest": {
    "zenbox/doctrine": "Provides Doctrine ORM pack"
  },
  "autoload": {
    "psr-4": {
      "ZenBox\\Ddd\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ZenBox\\Ddd\\Test\\": "test/",
      "ZenBox\\Ddd\\Example\\": "example/"
    }
  }
}
